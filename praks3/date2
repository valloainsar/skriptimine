#!/bin/bash
#
DAY=$(date +"%A, %B %d,%Y") # Defineeritud, mis on "Day"
TIME=$(date +"%H.%M") # Defineeritud, mis on "Time"

echo "Today is $DAY" #  Kuvab k채sureale eelnefa (DAY) definitsiooni
echo "Time is $TIME" # Kuvab k채sureale aja (eelenevalt defineeritud)
echo -e -n "Calender of" # Kirjutab kalendri kohale mis kuuga on tegu
cal # See k채sk kuvab kalendri
#
